#include <iostream>
using namespace std;

int main()

{
    cout << "Testing 1,2,3" << endl;
    cout << "Testing 4,5,6" << endl;
    //This is a placeholder
}

double mathFunction(double value1, string op, double value2)
{
    double answer = 0;
    
    if (op == "+")
    {
        answer = value1 + value2;
    }
    
    else if (op == "-")
    {
        answer = value1 - value2;
    }
    
    else if (op == "*")
    {
        answer = value1 * value2;        
    }
    else if (op == "/")
    {
        answer = value1 / value2;
    }
    return answer;
}
